#!/bin/bash

### Launch telnetd ###
if [ -f /sbin/telnetd ]; then
	/sbin/telnetd &
fi

### create symlink busyboxhm.elf as busybox ###
ln -s /bin/busyboxhm.elf /bin/busybox

### create symlink /proc/mounts as /etc/mtab ###
ln -s /proc/mounts /etc/mtab

### fsck filesystems in /etc/fstab
fsck

### mount all filesystems in /etc/fstab
mount -a

### mount android system, vendor and userdata images
if [ -d /odm ]; then
	sh /etc/mount.sh
fi

### Launch rc.local
sh /etc/rc.local
