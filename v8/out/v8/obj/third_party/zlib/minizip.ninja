defines = -DUSE_UDEV -DUSE_AURA=1 -DUSE_NSS_CERTS=1 -DUSE_OZONE=1 -DUSE_X11=1 -D_FILE_OFFSET_BITS=64 -D_LARGEFILE_SOURCE -D_LARGEFILE64_SOURCE -DCR_SYSROOT_HASH=2befe8ce3e88be6080e4fb7e6d412278ea6a7625 -DNDEBUG -DNVALGRIND -DDYNAMIC_ANNOTATIONS_ENABLED=0
include_dirs = -I../.. -Igen -I../../third_party/zlib
cflags = -fno-ident -fno-strict-aliasing --param=ssp-buffer-size=4 -fstack-protector -funwind-tables -fPIC -pipe -pthread -Wno-builtin-macro-redefined -D__DATE__= -D__TIME__= -D__TIMESTAMP__= -O2 -fdata-sections -ffunction-sections -fno-omit-frame-pointer -g0 -fvisibility=hidden -Wno-unused-local-typedefs -Wno-maybe-uninitialized -Wno-deprecated-declarations -Wno-comments -Wno-missing-field-initializers -Wno-unused-parameter
cflags_c = -std=gnu11 --sysroot=../../../../hmsdk/sysroots/aarch64-euler-elf_all_in_one/
label_name = minizip
target_out_dir = obj/third_party/zlib
target_output_name = libminizip

build obj/third_party/zlib/minizip/ioapi.o: cc ../../third_party/zlib/contrib/minizip/ioapi.c
build obj/third_party/zlib/minizip/unzip.o: cc ../../third_party/zlib/contrib/minizip/unzip.c
build obj/third_party/zlib/minizip/zip.o: cc ../../third_party/zlib/contrib/minizip/zip.c

build obj/third_party/zlib/libminizip.a: alink obj/third_party/zlib/minizip/ioapi.o obj/third_party/zlib/minizip/unzip.o obj/third_party/zlib/minizip/zip.o || obj/third_party/zlib/libchrome_zlib.a obj/third_party/zlib/zlib_adler32_simd.stamp obj/third_party/zlib/zlib_common_headers.stamp obj/third_party/zlib/zlib_inflate_chunk_simd.stamp obj/third_party/zlib/zlib_arm_crc32.stamp obj/third_party/zlib/zlib_x86_simd.stamp
  arflags = -T
  output_extension = .a
  output_dir = obj/third_party/zlib
